{
	"name": "homelink",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db-init-device-tyoe": "wrangler d1 execute DB --command 'drop table device_type' && wrangler d1 execute DB --file data/create-device-type.sql"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.6.5",
		"@tailwindcss/postcss": "^4.1.12",
		"next": "15.4.6",
		"postcss": "^8.5.6",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@types/node": "^20.19.11",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.31.0"
	}
}
